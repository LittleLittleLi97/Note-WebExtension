{
    "manifest_version": 3,
    "name": "web_collect",
    "description": "chrome extension",
    "version": "0.0.1",
    "permissions": [
        
    ],
    "background": {
        "service_worker": "/background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "icons": {
        "128": "/img/Icon-128.png"
    }
}